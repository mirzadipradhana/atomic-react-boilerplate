{
  "name": "atomic-react-boilerplate",
  "version": "0.1.0",
  "description": "React.JS atomic design boilerplate build by Webpack",
  "main": "src/server/main.js",
  "scripts": {
    "start": "npm run build && npm run production",
    "develop": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "production": "NODE_PATH=./src NODE_ENV=production node ./src/server/server.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js"
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Mirza Adipradhana",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-inline-environment-variables": "0.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-root-import": "^4.0.1",
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "compression": "^1.7.1",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.8.5",
    "history": "^4.7.2",
    "immutability-helper": "^2.1.2",
    "ip": "^1.1.3",
    "json-loader": "^0.5.4",
    "material-ui": "^0.19.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-mixins": "^5.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-simple-vars": "^4.1.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "ramda": "^0.25.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.14.3",
    "stats-webpack-plugin": "^0.4.2",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.7",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "redux-devtools": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
}
