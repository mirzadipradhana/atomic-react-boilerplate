{
  "presets": ["react", "es2015"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    [
      "babel-root-import",
      "babel-plugin-transform-inline-environment-variables",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-object-rest-spread",
      "react-hot-loader/babel"
    ]
  ]
}
